<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_tiempovascular21">

    <ImageView
        android:id="@+id/imgBackk"
        android:layout_width="52dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.017"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageView204"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.247"
        app:srcCompat="@drawable/fondoclaropequeno" />

    <ImageView
        android:id="@+id/imageView205"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView204"
        app:layout_constraintEnd_toEndOf="@+id/imageView204"
        app:layout_constraintStart_toStartOf="@+id/imageView204"
        app:layout_constraintTop_toTopOf="@+id/imageView204"
        app:layout_constraintVertical_bias="0.28"
        app:srcCompat="@drawable/abdominales2" />

    <TextView
        android:id="@+id/textView149"
        android:layout_width="341dp"
        android:layout_height="73dp"
        android:fontFamily="@font/lexend_black"
        android:text="REALIZA 10 FLEXIONES"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView204"
        app:layout_constraintEnd_toEndOf="@+id/imageView204"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/imageView204"
        app:layout_constraintTop_toTopOf="@+id/imageView204"
        app:layout_constraintVertical_bias="0.886" />

    <Button
        android:id="@+id/btnT21"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="156dp"
        android:layout_height="59dp"
        android:text="SIGUIENTE"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:backgroundTint="#7663F2"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.801"  />

</androidx.constraintlayout.widget.ConstraintLayout>