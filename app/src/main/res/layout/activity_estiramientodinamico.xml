<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_estiramientodinamico">

    <ImageButton
        android:id="@+id/imgbuttonregresarpausa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.247"
        app:srcCompat="@drawable/fondoclaropequeno"/>

    <ImageView
        android:id="@+id/img_siguientePausa20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.785"
        app:srcCompat="@drawable/fondomaspequenoclaro" />

    <ImageView
        android:id="@+id/imageView75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintStart_toStartOf="@+id/imageView73"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.27"
        app:srcCompat="@drawable/estiramientodinamico" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lexend_black"
        android:text="ESTIRAMIENTO DINAMICO "
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.923" />


    <TextView
        android:id="@+id/txtestiramientoRCV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lexend_black"
        android:text="01:00"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/img_siguientePausa20"
        app:layout_constraintEnd_toEndOf="@+id/img_siguientePausa20"
        app:layout_constraintStart_toStartOf="@+id/img_siguientePausa20"
        app:layout_constraintTop_toBottomOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgIniciar1min"
        android:layout_width="47dp"
        android:layout_height="37dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_siguientePausa20"
        app:layout_constraintTop_toBottomOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.875"
        app:srcCompat="@drawable/pausa_png"
        tools:layout_editor_absoluteX="302dp" />

    <ImageView
        android:id="@+id/imgplay"
        android:layout_width="57dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_siguientePausa20"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.985"
        app:srcCompat="@drawable/imgplay1"
        tools:layout_editor_absoluteX="232dp" />

</androidx.constraintlayout.widget.ConstraintLayout>