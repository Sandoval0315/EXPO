<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".activity_rutinas">


    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="1490dp"
        android:layout_height="1175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:layout_constraintVertical_bias="0.144"
        app:srcCompat="@drawable/fondoclaroredondo" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="305dp"
        android:layout_height="259dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rutinas" />

    <Button
        android:id="@+id/btnRutinaCardio"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="325dp"
        android:layout_height="116dp"
        android:fontFamily="@font/lexend_extrabold"
        android:text="RUTINAS                  CARDIOVASCULAR"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textColor="#3A3636"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#7663F2"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
