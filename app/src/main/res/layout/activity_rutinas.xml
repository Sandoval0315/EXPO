        app:srcCompat="@drawable/caminadora" />


    <Button
        android:id="@+id/btnAltaIntensidad"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="325dp"
        android:layout_height="116dp"
        android:fontFamily="@font/lexend_extrabold"
        android:text="Rutinas de                               Alta                                 Intensidad"
        android:textAlignment="viewStart"
        android:textColor="#3A3636"
        android:textSize="16sp"
        app:backgroundTint="#7663F2"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.816" />

    <Button
        android:id="@+id/btnFuerzaPesas"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_width="325dp"
        android:layout_height="116dp"
        android:fontFamily="@font/lexend_extrabold"
        android:text="Rutinas de                              Fuerzas con                            Pesas"
        android:textAlignment="viewStart"
        android:textColor="#3A3636"
        android:textSize="16sp"
        app:backgroundTint="#7663F2"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.509" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="88dp"
        android:layout_height="85dp"
