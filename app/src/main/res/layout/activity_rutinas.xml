        android:id="@+id/imageView8"
        android:layout_width="88dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnRunnig"
        app:layout_constraintEnd_toEndOf="@+id/btnRunnig"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="@+id/btnRunnig"
        app:layout_constraintTop_toTopOf="@+id/btnRunnig"
        app:layout_constraintVertical_bias="0.478"
        app:srcCompat="@drawable/runnig" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="88dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnAltaIntensidad"
